meme_dict = {
            "CRINGE": "Algo excepcionalmente raro o embarazoso",
            "LOL": "Una respuesta común a algo gracioso",
            "ROFL":"Una respuesta a una broma",
            "SHEESH":"Ligera desapobración",
            "CREEPY":"Aterrados,siniestro",
            "AGGRO":"Ponerse agresivo/enojado",
            }
            
word = input("Escribe una palabra que no entiendas (¡con mayúsculas!): ") 
if word in meme_dict.keys():
    print("EXISTE")
    if word=="LOL":
        print("Una respuesta comun a algo gracioso")
       if word=="CRINGE":
        print("Algo raro o embarazoso")
        if word=="ROFL":
        print("Una respuesta a una broma")
        if word=="SHEESH":
        print("Ligera desaprobación")
        if word=="CREEPY":
        print("Alterrador,siniestro")
        if word=="AGGRO":
        print("Ponerse agresivo/enojado")
        
if word in meme_dict.keys():
    # ¿Qué debemos hacer si se encuentra la palabra?
    print:("EXISTE")
else: 
    # ¿Qué hacer si no se encuentra la palabra?
    print:("NO EXISTE")
            
            
            
            
          
